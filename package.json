{
    "name": "structural-motion",
    "displayName": "Structural Motion",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "structural-motion.moveStructureDown",
                "title": "Move Structure Down",
                "enablement": "activeEditor && !editorReadonly"
            },
            {
                "command": "structural-motion.moveStructureUp",
                "title": "Move Structure Up",
                "enablement": "activeEditor && !editorReadonly"
            }
        ],
        "keybindings": [
            {
                "command": "structural-motion.moveStructureDown",
                "key": "ctrl+shift+down",
                "when": "editorTextFocus",
                "mac": "cmd+shift+down"
            },
            {
                "command": "structural-motion.moveStructureUp",
                "key": "ctrl+shift+up",
                "when": "editorTextFocus",
                "mac": "cmd+shift+up"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts --max-warnings=0",
        "test": "npm run compile && vscode-test",
        "format:check": "prettier . --check",
        "format:fix": "prettier . --write",
        "check": "npm run lint && npm run format:check && npm run test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.14.2",
        "@types/vscode": "^1.90.0",
        "@typescript-eslint/eslint-plugin": "^7.13.0",
        "@typescript-eslint/parser": "^7.13.0",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.4.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "^3.3.2",
        "typescript": "^5.4.5"
    }
}
